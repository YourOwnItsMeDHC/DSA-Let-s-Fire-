class Solution {
    public int minCostToMoveChips(int[] position) {
          int coinsAtEven = 0, coinsAtOdd = 0;
        
          for(int i=0; i<position.length; i++) {
              if(position[i] % 2 !=0) {
                 ++coinsAtOdd;
              }
              else {
                  ++coinsAtEven;
              }
         }
        return Math.min(coinsAtEven , coinsAtOdd);
    }
}