//Optimal Approach : Two Pointer (Faster than 100%)
class Solution {
    public int removeDuplicates(int[] nums) {
        if(nums.length <= 0) {          //Check whether ,  there is any element or not
            return nums.length;
        }
        
        int i = 0;
        for(int j=1; j<nums.length; j++) {
            if(nums[i] != nums[j]) 
                i++;
            nums[i] = nums[j];
        }
        return ++i;                       //or i+1
     }
}


