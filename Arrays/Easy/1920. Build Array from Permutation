class Solution {
    public int[] buildArray(int[] nums) {
        int[] ans = new int[nums.length];
        for(int i=0; i<nums.length; i++) {
            // if(nums[i] >= nums.length || nums[i] < 0) {
            //     break;
            // }
            ans[i] = nums[nums[i]];
        }
        return ans;
    }
}